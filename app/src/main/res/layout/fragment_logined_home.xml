<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".Fragment.HomeLoginedFragment">

    <include
        android:id="@+id/include11"
        layout="@layout/logo_helo_user"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:layout_marginTop="-20dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <ImageView
        android:id="@+id/imageView5"
        android:layout_width="match_parent"
        android:layout_height="260dp"
        android:scaleType="centerCrop"
        android:src="@drawable/canh_chim_nen"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:srcCompat="@drawable/canh_chim_nen" />

    <Button
        android:id="@+id/btn_home_1"
        android:layout_width="350dp"
        android:layout_height="80dp"
        android:background="@drawable/btn_home_1"
        android:paddingLeft="5dp"
        android:paddingBottom="5dp"
        android:text="@string/btn_home_1"
        android:textAllCaps="false"
        android:textColor="@color/white"
        android:textScaleX="0.9"
        android:textSize="18sp"
        app:layout_constraintBottom_toTopOf="@+id/btn_home_2"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.496"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/include11" />

    <Button
        android:id="@+id/btn_home_2"
        android:layout_width="350dp"
        android:layout_height="80dp"
        android:background="@drawable/btn_home_2"
        android:paddingLeft="5dp"
        android:paddingBottom="5dp"
        android:text="@string/btn_home_2"
        android:textAllCaps="false"
        android:textColor="@color/white"
        android:textScaleX="0.9"
        android:textSize="18sp"
        app:layout_constraintBottom_toTopOf="@+id/btn_home_3"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/btn_home_1" />

    <Button
        android:id="@+id/btn_home_3"
        android:layout_width="350dp"
        android:layout_height="80dp"
        android:background="@drawable/btn_home_3"
        android:paddingLeft="5dp"
        android:paddingBottom="5dp"
        android:text="@string/btn_home_3"
        android:textAllCaps="false"
        android:textColor="@color/white"
        android:textScaleX="0.9"
        android:textSize="18sp"
        app:layout_constraintBottom_toTopOf="@+id/btn_home_4"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.493"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/btn_home_2" />

    <Button
        android:id="@+id/btn_home_4"
        android:layout_width="350dp"
        android:layout_height="80dp"
        android:background="@drawable/btn_home_4"
        android:paddingLeft="5dp"
        android:paddingBottom="5dp"
        android:text="@string/btn_home_4"
        android:textAllCaps="false"
        android:textColor="@color/white"
        android:textScaleX="0.9"
        android:textSize="18sp"
        app:layout_constraintBottom_toTopOf="@+id/btn_home_5"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.493"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/btn_home_3" />

    <Button
        android:id="@+id/btn_home_5"
        android:layout_width="350dp"
        android:layout_height="80dp"
        android:background="@drawable/btn_home_5"
        android:paddingLeft="5dp"
        android:paddingBottom="5dp"
        android:text="@string/btn_home_5"
        android:textAllCaps="false"
        android:textColor="@color/white"
        android:textScaleX="0.9"
        android:textSize="18sp"
        app:layout_constraintBottom_toTopOf="@+id/btn_QR_Code"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.493"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/btn_home_4" />

    <Button
        android:id="@+id/btn_QR_Code"
        android:layout_width="68dp"
        android:layout_height="68dp"
        android:background="@drawable/btn_qr"
        android:paddingLeft="5dp"
        android:paddingBottom="5dp"
        android:textAllCaps="false"
        android:textColor="@color/white"
        android:textScaleX="0.9"
        app:layout_constraintBottom_toTopOf="@+id/textView28"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.493"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/btn_home_5" />

    <TextView
        android:id="@+id/textView28"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="8dp"
        android:fontFamily="@font/myriadpro_semiext"
        android:text="Quét mã QR"
        android:textColor="@color/textColorGreen"
        android:textSize="16sp"
        app:layout_constraintBottom_toBottomOf="@+id/imageView5"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/btn_QR_Code" />

</androidx.constraintlayout.widget.ConstraintLayout>